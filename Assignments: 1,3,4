        "def calcTuition(credits):\n",
        "    if credits >= 12:\n",
        "        tuition = 20000\n",
        "    else:\n",
        "        tuition = 1200 + (1700 * credits)\n",
        "    return tuition\n",
        "\n",
        "def main():\n",
        "  credits = int(input(\"Enter the number of credits you are taking: \"))\n",
        "  tuition = calcTuition(credits)\n",
        "  print(f\"Your tuition for the semester is: ${tuition}\")\n",
        "\n",
        "main()"
