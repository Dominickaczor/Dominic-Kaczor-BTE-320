      "source": [
        "def getParkingHours():\n",
        "  return int(input('Enter number of hours: '))\n",
        "\n",
        "def calcParkingFee(hour):\n",
        "  fee = 5 + 2.5 * hour\n",
        "  if fee < 6.0:\n",
        "    return 6.0\n",
        "  elif fee > 20.0:\n",
        "    return 20.0\n",
        "  else:\n",
        "    return fee\n",
        "\n",
        "def main():\n",
        "  hours = int(input('Enter number of hours: '))\n",
        "  fee = calcParkingFee(hours)\n",
        "  print(fee)\n",
        "\n",
        "main()"
